        "openid",
        "profile",
        "email",
        "web-origins",
        "role_list",
        "roles",
        "inventory",
        "sales",
        "notifications"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt",
        "reports",
        "audit"
      ],
      "access": {
        "view": true,
        "configure": true,
        "manage": true
      }
